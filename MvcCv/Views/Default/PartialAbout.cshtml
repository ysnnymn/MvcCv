@model List<MvcCv.Models.Entity.TblAbout>
<section class="resume-section" id="about">
    <div class="resume-section-content">
        @foreach (var item in Model)
        {
            <h1 class="mb-0">
                @item.Name
                <span class="text-primary">@item.SurName</span>
            </h1>

            <div class="subheading mb-5">
                @item.Adress
                <a href="mailto:@item.Email">@item.Email</a>
            </div>
            <p class="lead mb-5">@item.Description</p>
        }

        <div class="social-icons">
            <a class="social-icon" href="https://www.linkedin.com/in/yasin-yaman-b1a054218/" target="_blank"><i class="fab fa-linkedin-in"></i></a>
            <a class="social-icon" href="https://github.com/ysnnymn"target="_blank"><i class="fab fa-github"></i></a>
            <a class="social-icon" href="https://twitter.com/YasinYa95252103"target="_blank"><i class="fab fa-twitter"></i></a>
            <a class="social-icon" href="https://www.facebook.com/profile.php?id=100011853074458"target="_blank"><i class="fab fa-facebook-f"></i></a>
        </div>
    </div>
</section>
